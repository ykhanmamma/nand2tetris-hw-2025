/**
 * Demultiplexor:
 * [a, b] = [in, 0] if sel = 0
 *          [0, in] if sel = 1
 */
CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Nand(a=sel, b=sel, out=notsel);  
    Nand(a=in, b=notsel, out=aout);  
    Nand(a=in, b=sel, out=bout); 
    Nand(a=aout, b=aout, out=a);  
    Nand(a=bout, b=bout, out=b); 
}
