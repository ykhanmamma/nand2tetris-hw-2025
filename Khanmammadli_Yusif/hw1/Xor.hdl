/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a , out=nouta );
    Not(in= b, out=noutb );
    Or(a=nouta , b=noutb , out=orout );
    Or(a=a , b=b , out=normorout );
    And(a=orout , b=normorout , out=out );
}