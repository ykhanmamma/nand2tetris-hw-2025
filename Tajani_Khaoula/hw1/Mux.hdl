/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;
    PARTS:
    Not(in=sel, out=nSel);
    And(a=a, b=nSel, out=outA);
    And(a=b, b=sel, out=outB);
    Or(a=outA, b=outB, out=out);
}
