/**
 * Demultiplexor:
 * [a, b] = [in, 0] if sel = 0
 *          [0, in] if sel = 1
 */
CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Nand(a=sel , b=sel , out=notSel);
    Nand(a=in , b=notSel , out=aNand); 
    Nand(a=aNand , b=aNand , out=a);
    Nand(a=in , b=sel , out=bNand);
    Nand(a=bNand , b=bNand , out=b);
}