/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
  CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
	Nand(a=sel, b=sel, out=nsel);
	Nand(a=a,b=nsel,out=ansel);
	Nand(a=b, b=sel, out=bsel);
	Nand(a=ansel, b=bsel,out=out);     
     
}
