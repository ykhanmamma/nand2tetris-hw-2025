/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in= a, out= na);
    Not(in= b, out= nb);
    And(a= na, b= nb, out= t1);
    Not(in= t1, out= out);
}